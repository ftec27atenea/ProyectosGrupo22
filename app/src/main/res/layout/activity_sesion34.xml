<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sesion34">
    <TextView
        android:id="@+id/Nombret"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="156dp" />

    <TextView
        android:id="@+id/Alturat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Altura"
        app:layout_constraintTop_toBottomOf="@+id/Nombret"
        tools:layout_editor_absoluteX="177dp" />

    <TextView
        android:id="@+id/Estudiantet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Estudiante"
        app:layout_constraintTop_toBottomOf="@+id/Alturat"
        tools:layout_editor_absoluteX="178dp" />

    <TextView
        android:id="@+id/Edadt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="Edad"
        app:layout_constraintTop_toBottomOf="@+id/Estudiantet"
        tools:layout_editor_absoluteX="179dp" />

</androidx.constraintlayout.widget.ConstraintLayout>